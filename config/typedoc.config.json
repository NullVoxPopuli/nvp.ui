{
  "$schema": "https://typedoc.org/schema.json",
  "tsconfig": "./tsconfig.typedoc.json",
  "cleanOutputDir": true,
  "compilerOptions": {
    "noEmitOnError": false
  },
  "exclude": ["../dist/_app_/", "../dist/**/*.js", "../dist/**/*.js.map", "../dist/**/*.d.ts.map"],
  "out": "../dist-docs",
  "json": "../dist-docs/docs.json",
  "pretty": true,
  "excludeInternal": false,
  "skipErrorChecking": true,
  "plugin": []
}
