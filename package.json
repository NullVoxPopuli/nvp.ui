{
  "name": "nvp.ui",
  "version": "0.4.0",
  "description": "UI Kit for building professional and sleek looking ember apps quickly",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NullVoxPopuli/nvp.ui.git"
  },
  "license": "MIT",
  "author": "nullvoxpopuli",
  "type": "module",
  "imports": {
    "#app/*": "./docs-app/*",
    "#src*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./declarations/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./declarations/components/*.d.ts",
      "default": "./dist/components/*.js"
    },
    "./test-support": {
      "types": "./declarations/test-support/index.d.ts",
      "default": "./dist/test-support/index.js"
    },
    "./addon-main": "./config/addon-main.cjs"
  },
  "files": [
    "dist",
    "declarations",
    "src",
    "config/addon-main.cjs"
  ],
  "scripts": {
    ":rollup": "rollup --config ./config/rollup.config.mjs",
    ":testem": "testem --file ./config/testem.cjs",
    ":vite": "vite --config ./config/vite.config.mjs",
    "build": "pnpm :rollup",
    "build:api-docs": "typedoc --options ./config/typedoc.config.json --json ./dist-docs/docs.json ",
    "build:app": "pnpm :vite build --out-dir dist-app",
    "build:tests": "pnpm :vite build --mode=development --out-dir dist-tests",
    "deploy:prepare": "pnpm build && pnpm build:api-docs && pnpm build:app",
    "deploy:preview": "pnpm deploy:prepare && pnpm :vite preview --outDir dist-app",
    "format": "prettier . --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm run format",
    "lint:format": "prettier . --check",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:package": "pnpm publint",
    "lint:published-types": "attw *.tgz || exit 0",
    "lint:types": "ember-tsc --noEmit",
    "prepack": "pnpm :rollup",
    "prepare": "pnpm build && pnpm build:api-docs",
    "start": "pnpm :vite dev",
    "start:api-docs": "npx html-pages ./dist-docs --no-cache",
    "start:docs": "pnpm :vite preview --outDir dist-app",
    "test": "pnpm build:tests && pnpm test:ci",
    "test:ci": "pnpm :testem ci",
    "test:launch": "pnpm :testem server"
  },
  "dependencies": {
    "@ember/test-helpers": "^5.4.1",
    "@ember/test-waiters": ">= 4.1.0",
    "@embroider/addon-shim": "^1.10.2",
    "@embroider/macros": "1.19.5",
    "@glimmer/component": "^2.0.0",
    "@glint/template": "^1.7.3",
    "decorator-transforms": "^2.3.0",
    "ember-modifier": ">= 4.2.0",
    "ember-primitives": "^0.48.1",
    "ember-resources": ">= 7.0.4"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.1",
    "@babel/core": "^7.28.5",
    "@babel/helpers": "^7.27.1",
    "@ember/app-tsconfig": "^2.0.0",
    "@ember/library-tsconfig": "^2.0.0",
    "@ember/render-modifiers": "^3.0.0",
    "@embroider/addon-dev": "8.1.2",
    "@embroider/compat": "^4.1.11",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.4.1",
    "@embroider/legacy-inspector-support": "^0.1.3",
    "@embroider/try": "github:NullVoxPopuli/try#768631a",
    "@embroider/vite": "^1.4.4",
    "@glint/ember-tsc": "^1.0.8",
    "@glint/tsserver-plugin": "^2.0.8",
    "@nullvoxpopuli/eslint-configs": "^5.4.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@shikijs/rehype": "^3.20.0",
    "@tsconfig/ember": "^3.0.12",
    "@types/qunit": "^2.19.12",
    "@types/rsvp": "^4.0.9",
    "@zamiell/typedoc-plugin-not-exported": "^0.3.0",
    "babel-plugin-ember-template-compilation": "^3.0.1",
    "change-case": "^5.4.4",
    "concurrently": "^9.1.2",
    "content-tag": "^3.1.3",
    "ember-a11y-testing": "^7.1.3",
    "ember-focus-trap": "^1.1.1",
    "ember-mobile-menu": "^6.0.0",
    "ember-modifier": ">= 4.2.0",
    "ember-on-resize-modifier": "^2.0.2",
    "ember-page-title": "^9.0.1",
    "ember-qunit": "^9.0.4",
    "ember-repl": "^7.0.1",
    "ember-resize-observer-service": "^1.1.0",
    "ember-resolver": "^13.1.0",
    "ember-route-template": "^1.0.3",
    "ember-scoped-css": "^2.0.4",
    "ember-set-body-class": "^1.0.2",
    "ember-source": "6.9.0",
    "ember-template-imports": "^4.3.0",
    "ember-template-lint": "^7.6.0",
    "eslint": "^9.39.2",
    "execa": "^9.6.1",
    "highlight.js": "^11.11.1",
    "highlightjs-glimmer": "^2.2.2",
    "kolay": "^3.6.2",
    "lodash.sortby": "^4.7.0",
    "lorem-ipsum": "^2.0.8",
    "prettier": "^3.7.4",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "publint": "^0.3.16",
    "qunit": "^2.24.3",
    "qunit-assertions-extra": "^1.0.3",
    "qunit-dom": "^3.4.0",
    "reactiveweb": "^1.4.2",
    "release-plan": "^0.17.2",
    "rollup": "~4.53.3",
    "rollup-plugin-copy": "^3.5.0",
    "shiki": "^3.20.0",
    "testem": "^3.17.0",
    "tracked-built-ins": "^4.0.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "unplugin-info": "^1.2.2",
    "vite": "^7.2.7"
  },
  "packageManager": "pnpm@10.10.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "pnpm": {
    "overrides": {
      "@glint/template": "$@glint/template",
      "@glimmer/template": "$@glimmer/component",
      "ember-modifier": "$ember-modifier"
    }
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "config/addon-main.cjs",
    "app-js": {}
  }
}
